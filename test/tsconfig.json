{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["ES2019"],
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "types": [
      "vitest/globals",
      "unplugin-vue-prop"
    ],
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "typeRoots": ["./types"]
  },
  "include": ["./"],
  "exclude": []
}
