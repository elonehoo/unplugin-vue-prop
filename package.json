{
  "name": "unplugin-vue-prop",
  "type": "module",
  "version": "0.0.10",
  "description": "Add import define props type support for Vue3 script-setup and lang is typescript",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/unplugin-vue-prop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/unplugin-vue-prop.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/unplugin-vue-prop/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "nuxt"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup",
    "dev": "cross-env NODE_ENV=development tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "play:vue": "npm -C playground/vue run dev",
    "play:nuxt": "npm -C playground/nuxt run dev",
    "play:vue2": "npm -C playground/vue2 run dev",
    "play:webpack": "npm -C playground/webpack run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "test:ci": "cross-env CI=true vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": ">3.2.0"
  },
  "dependencies": {
    "@babel/types": "^7.22.5",
    "@vue/compiler-sfc": "^3.3.4",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.0",
    "local-pkg": "^0.4.3",
    "magic-string": "^0.30.1",
    "picocolors": "^1.0.0",
    "unplugin": "^1.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@types/debug": "^4.1.8",
    "@types/node": "^18.16.19",
    "@vitest/coverage-c8": "^0.32.4",
    "@vitest/ui": "^0.32.4",
    "bumpp": "^9.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "esno": "^0.16.3",
    "rollup": "^3.26.0",
    "tsup": "6.7.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.4",
    "vue": "^3.3.4",
    "webpack": "^5.88.1"
  }
}
