{
  "name": "unplugin-vue-prop",
  "version": "0.0.2",
  "description": "Add import define props type support for Vue3 script-setup and lang is typescript",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/unplugin-vue-prop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/unplugin-vue-prop.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/unplugin-vue-prop/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env rimraf dist && tsup && tsx scripts/postbuild.mts",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@babel/generator": "^7.20.7",
    "@babel/parser": "^7.20.7",
    "@rollup/pluginutils": "^5.0.2",
    "@vue/compiler-core": "^3.2.45",
    "@vue/compiler-dom": "^3.2.45",
    "@vue/compiler-sfc": "^3.2.45",
    "magic-string": "^0.27.0",
    "typescript": "^4.9.4",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@sxzz/eslint-config-prettier": "^2.4.4",
    "@sxzz/eslint-config-vue": "^2.4.4",
    "@types/babel__generator": "^7.6.4",
    "@types/node": "^18.11.18",
    "@vitest/ui": "^0.26.2",
    "bumpp": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-define-config": "^1.12.0",
    "fast-glob": "^3.2.12",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.0",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "unplugin-vue": "^4.0.1",
    "vite": "^4.0.3",
    "vitest": "^0.26.2",
    "vue": "^3.2.45"
  }
}
