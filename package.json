{
  "name": "unplugin-vue-prop",
  "version": "0.0.6",
  "description": "Add import define props type support for Vue3 script-setup and lang is typescript",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/unplugin-vue-prop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/unplugin-vue-prop.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/unplugin-vue-prop/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "nuxt"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup",
    "dev": "cross-env NODE_ENV=development tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "test:ci": "cross-env CI=true vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": ">3.2.0"
  },
  "dependencies": {
    "@babel/types": "^7.21.2",
    "@vue/compiler-sfc": "^3.2.47",
    "debug": "^4.3.4",
    "fast-glob": "^3.2.12",
    "local-pkg": "^0.4.3",
    "magic-string": "^0.30.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.14.2",
    "@vitest/coverage-c8": "^0.29.1",
    "@vitest/ui": "^0.29.1",
    "bumpp": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "esno": "^0.16.3",
    "tsup": "6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue": "^3.2.47"
  }
}
